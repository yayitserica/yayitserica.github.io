/* tabtabHover and tabtabActive should really be
   tabtab:hover and tabtab:active. However, IE
   does not support these pseudo class on TD elements.
   This hack is used to get around it. */


td.tabtab          {cursor:pointer}
td.tabtabHover     {cursor:pointer;background-color:yellow}
td.tabtabActive    {cursor:pointer}

td.tabfiller       {}
td.tabdtime        {}
td.tabclock        {}

