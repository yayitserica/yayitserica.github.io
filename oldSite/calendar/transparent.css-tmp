/*=============================================================================*
 Body
 =============================================================================*/
body      {font-family: "comic sans ms", arial; 
           margin:0 2px 10px 2px;
	   background-color: #fff;
           background-image: url("/calendar/images/puppy.jpg");
           background-repeat: no-repeat;
          /* background-attachment:fixed; */
          } 
td        {font-family: "comic sans ms"; }
 
/*=============================================================================*
 Link
 =============================================================================*/
a         { color:#33c; text-decoration:none}
a:visited { color:#33c; text-decoration:none}
a:active  { color:#33F; text-decoration:underline}
a:hover   { color:#FF3036; text-decoration:none;color:red;background-color:#0f0}
a.transparent:hover {background-color:transparent}


